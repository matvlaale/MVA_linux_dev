#: guess.c:125
#, c-format
msgid "Usage: guess [params]\n"
msgstr "Использование: guess [параметры]\n"

#: guess.c:126
#, c-format
msgid "Parameters:\n"
msgstr "Параметры:\n"

#: guess.c:127
#, c-format
msgid "  -r          Use Roman numbers\n"
msgstr "  -r          Использовать римские числа\n"

#: guess.c:128
#, c-format
msgid "  --help      Show this help message\n"
msgstr "  --help      Показать данное сообщение\n"
